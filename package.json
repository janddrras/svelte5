		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.13.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.2",
		"svelte": "^5.13.0",
		"svelte-check": "^4.1.1",
		"tailwindcss": "^3.4.16",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.18.0",
